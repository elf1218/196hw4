<table>
    <tr>
        <td>time</td>
        <td>description</td>
        <td>user</td>
        <td>calendar</td>
    </tr>
    <% @events.each do |event| %>
    <tr>
        <td><%= event.time %></td>
        <td><%= link_to event.desc, event_path(event.id) %></td>
        <td><%= event.user.email %></td>
        <td><%= link_to event.calendar.title, calendar_path(event.calendar.id) %></td>
    </tr>
    <% end %>
</table>